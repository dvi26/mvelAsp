<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="mvelMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:mvelMaui.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="mvelMaui">

    <FlyoutItem Title="Menú">
        <Tab>
            <ShellContent Title="Puntuar Combates" ContentTemplate="{DataTemplate local:puntuarCombate}"/>
            <ShellContent Title="Ver Clasificación" ContentTemplate="{DataTemplate local:verClasificacion}"/>
        </Tab>
    </FlyoutItem>
</Shell>
